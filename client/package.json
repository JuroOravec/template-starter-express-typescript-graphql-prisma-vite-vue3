{
  "name": "web-app-client",
  "private": true,
  "author": "Juro Oravec <juraj.oravec.josefson@gmail.com>",
  "homepage": "https://github.com/jurooravec/yudise-site#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jurooravec/yudise-site.git"
  },
  "bugs": {
    "url": "https://github.com/jurooravec/yudise-site/issues"
  },
  "scripts": {
    "nuxt:prd": "dotenv -e .env.prd -- nuxt",
    "nuxt:dev": "dotenv -e .env.dev -- nuxt",
    "build:ssg": "npm run nuxt:prd -- generate",
    "build:ssr": "npm run nuxt:prd -- build",
    "start:ssr": "npm run nuxt:prd -- start",
    "dev": "npm run dev:dev",
    "dev:dev": "npm run nuxt:dev -- dev",
    "dev:prd": "npm run nuxt:dev -- dev",
    "preview:dev": "npm run nuxt:dev -- preview",
    "preview:prd": "npm run nuxt:prd -- preview",
    "postinstall": "npm run nuxt:prd prepare",
    "test": "npm run test:unit",
    "test:unit": "jest --runInBand --forceExit --detectOpenHandles",
    "codegen": "run-s nuxt:gen graphql:gen",
    "nuxt:gen": "npm run nuxt:dev -- prepare",
    "graphql:gen": "graphql-codegen --config codegen.ts --verbose"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^5.0.0",
    "@graphql-codegen/cli": "^4.0.0",
    "@graphql-codegen/fragment-matcher": "^5.0.0",
    "@graphql-codegen/introspection": "^4.0.0",
    "@graphql-codegen/named-operations-object": "^2.3.1",
    "@graphql-codegen/time": "^5.0.0",
    "@graphql-codegen/typescript-apollo-client-helpers": "^2.2.6",
    "@graphql-codegen/typescript-operations": "^4.0.0",
    "@graphql-codegen/typescript-vue-apollo": "^3.3.7",
    "@invictus.codes/nuxt-vuetify": "^0.2.20",
    "@nuxt/devtools": "^0.5.3",
    "@nuxt/image": "^0.7.1",
    "@nuxtjs/apollo": "^5.0.0-alpha.6",
    "@nuxtjs/color-mode": "^3.2.0",
    "@nuxtjs/eslint-module": "^4.0.2",
    "@nuxtjs/html-validator": "^1.2.5",
    "@nuxtjs/i18n": "^8.0.0-beta.12",
    "@swc/core": "^1.3.64",
    "@swc/helpers": "^0.5.1",
    "@types/lodash": "^4.14.194",
    "@types/mixpanel-browser": "^2.38.1",
    "@types/node": "^18",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "npm-run-all": "^4.1.5",
    "nuxt": "^3.5.2",
    "nuxt-typed-router": "^3.2.2",
    "nuxt-vitest": "^0.6.12",
    "prettier": "^2.8.8",
    "regenerator-runtime": "^0.13.11",
    "serve": "^14.2.0",
    "ts-node": "^10.9.1",
    "vue-eslint-parser": "^9.3.0"
  },
  "dependencies": {
    "@codemirror/lang-javascript": "^6.1.9",
    "@codemirror/theme-one-dark": "^6.1.2",
    "@sentry/vue": "^7.54.0",
    "@vue/apollo-composable": "^4.0.0-beta.5",
    "@vue/apollo-util": "^4.0.0-beta.5",
    "@vuelidate/core": "^2.0.2",
    "@vuelidate/validators": "^2.0.2",
    "codemirror": "^6.0.1",
    "countries-list": "^2.6.1",
    "lodash": "^4.17.21",
    "mixpanel-browser": "^2.47.0",
    "sass": "^1.62.1",
    "vue": "^3.3.4",
    "vue-codemirror": "^6.1.1",
    "vuetify": "^3.3.1"
  },
  "overrides": {
    "@invictus.codes/nuxt-vuetify": {
      "vuetify": "$vuetify"
    }
  }
}
